    "prepack": "yarn clean && yarn build",
    "test": "yarn lint && yarn tsc && yarn format",
    "tsc": "tsc"
  },
  "keywords": [
    "yarn",
    "berry",
    "plugin",
    "nolyfill"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "license": "MIT",
  "dependencies": {
    "@yarnpkg/core": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@yarnpkg/builder": "^4.0.0",
    "husky": "^9.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^5.4.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wojtekmaj/yarn-plugin-nolyfill.git"
  },
  "funding": "https://github.com/wojtekmaj/yarn-plugin-nolyfill?sponsor=1",
